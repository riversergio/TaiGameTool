{
  "name": "taigametool",
  "version": "0.1.0",
  "description": "A Blogger Poster for TaiGame.Top",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --inspect app.js",
    "sass": "node-sass -w --output-style compress src/sass -o public/css",
    "prefix": "postcss public/css/*.css --use autoprefixer -b 'last 5 versions' -d public/css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riversergio/TaiGameTool.git"
  },
  "keywords": [
    "tool",
    "taigametop",
    "bloggerposter"
  ],
  "author": "River Sergio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/riversergio/TaiGameTool/issues"
  },
  "homepage": "https://github.com/riversergio/TaiGameTool#readme",
  "dependencies": {
    "express": "^4.17.1",
    "googleapis": "^67.0.0",
    "lowdb": "^1.0.0",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "postcss-cli": "^8.3.1"
  }
}
